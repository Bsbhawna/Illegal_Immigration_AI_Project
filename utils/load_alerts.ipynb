    "import pymongo\n",
    "\n",
    "def load_alerts():\n",
    "    \"\"\"Load alerts from MongoDB.\"\"\"\n",
    "    client = pymongo.MongoClient(\"mongodb://localhost:27017/\")\n",
    "    db = client[\"immigration_monitoring\"]\n",
    "    collection = db[\"alerts\"]\n",
    "    alerts = list(collection.find({}, {\"_id\": 0}))\n",
    "    return alerts\n"
   ]
  }
